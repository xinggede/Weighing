<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.widget.QMUIWindowInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/app_content_bg_color"
    android:clipChildren="false"
    app:qmui_skin_background="?attr/app_skin_main_background">

    <com.qmuiteam.qmui.widget.QMUITopBarLayout
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:gravity="center_horizontal"
        android:layout_marginTop="?attr/qmui_topbar_height"
        android:orientation="vertical">

        <Space
            android:layout_width="wrap_content"
            android:layout_height="40dp"/>

        <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
            android:id="@+id/bt_weight_print"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:gravity="center"
            android:padding="10dp"
            android:text="磅单打印"
            android:textSize="25sp"
            app:qmui_backgroundColor="?attr/app_skin_btn_test_bg"
            app:qmui_borderColor="@color/app_color_blue"
            app:qmui_borderWidth="0px"
            app:qmui_radius="10dp" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="30dp"/>

        <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
            android:id="@+id/bt_outbound_print"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:gravity="center"
            android:padding="10dp"
            android:text="出库单"
            android:textSize="25sp"
            app:qmui_backgroundColor="?attr/app_skin_btn_test_bg"
            app:qmui_borderColor="@color/app_color_blue"
            app:qmui_borderWidth="0px"
            app:qmui_radius="10dp" />

    </LinearLayout>

</com.qmuiteam.qmui.widget.QMUIWindowInsetLayout>
